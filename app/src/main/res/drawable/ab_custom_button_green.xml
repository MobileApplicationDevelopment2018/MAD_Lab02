<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#689F38" />
            <gradient android:angle="-90" android:endColor="#689F38" android:startColor="#689F38" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#689F38" />
            <solid android:color="#689F38" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <color android:color="#C5E1A5" />
        <stroke android:width="1dip" android:color="#9CCC65" />
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#9CCC65" />
            <gradient android:angle="-90" android:endColor="#689F38" android:startColor="#689F38" />
        </shape>
    </item>
</selector>